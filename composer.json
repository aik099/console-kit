{
	"name": "console-helpers/console-kit",
	"description": "Toolkit for creating command line applications using Symfony Console component",
	"license": "BSD 3-Clause",
	"authors": [
		{
			"name": "Alexander Obuhovich",
			"email": "aik.bold@gmail.com"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/console": "^2.7",
		"pimple/pimple": "^2.0|^3.0",
		"stecman/symfony-console-completion": "^0.6.1"
	},
	"require-dev": {
		"aik099/coding-standard": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"ConsoleHelpers\\ConsoleKit\\": "src/ConsoleKit/",
			"Tests\\ConsoleHelpers\\ConsoleKit\\": "tests/ConsoleKit/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.0.x-dev"
		}
	}
}
